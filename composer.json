{
    "name": "ethandevelter/laravel-shared-module",
    "description": "Shared Laravel backend logic and Vue components",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
        "Adalace\\SharedModule\\": "src/"
        }
    },
    "extra": {
        "laravel": {
        "providers": [
            "Adalace\\SharedModule\\SharedModuleServiceProvider"
        ]
        }
    },
    "require-dev": {
        "illuminate/support": "^12.16"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ethandevelter/laravel-shared-module.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
